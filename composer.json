    "require-dev": {
        "drupal/search_kint": "^1.0",
        "drupal/console" : "^1.8",
        "drupal/devel": "^1.2"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "preferred-install": "dist",
        "autoloader-suffix": "Drupal8",
        "vendor-dir": "docroot/vendor"
    },
    "scripts": {
        "post-install-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
            "cp -a docroot/profiles/contrib/sitefarm_seed/libraries/. docroot/libraries/"
        ],
        "post-update-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
            "cp -a docroot/profiles/contrib/sitefarm_seed/libraries/. docroot/libraries/"
        ]
    },
    "extra": {
        "_readme": [
            "By default Drupal loads the autoloader from ./vendor/autoload.php.",
            "To change the autoloader you can edit ./autoload.php."
        ],
        "merge-plugin": {
            "include": [
                "docroot/profiles/contrib/sitefarm_seed/composer.json"
            ],
            "replace": false,
            "merge-extra": true,
            "merge-extra-deep": false,
            "merge-scripts": true
        },
        "installer-types": [
            "drupal-custom-module",
            "drupal-custom-theme",
            "drupal-library",
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": ["type:drupal-core"],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/modules/custom/{$name}": ["type:drupal-custom-module"],
            "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
            "docroot/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "docroot/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "docroot/drush/contrib/{$name}": ["type:drupal-drush"],
            "docroot/libraries/{$name}": [
                "type:bower-asset",
                "type:npm-asset",
                "type:drupal-library"
            ]
        },
        "enable-patching": true,
        "patches" : {
            "drupal/core" : {
              "2644468-88 - Multiple image upload breaks image dimensions" :
              "https://www.drupal.org/files/issues/2644468-88.patch"
            },
            "drupal/fieldblock": {
              "2810783 - PHP exception when placing block":
              "https://www.drupal.org/files/issues/php_exception_when-2810783-30.patch"
            },
            "drupal/field_group": {
              "2652642 - Allow to position the group in the advanced sidebar column":
              "https://www.drupal.org/files/issues/2652642-33.patch"
            },
            "drupal/maxlength": {
              "2794421 - Add Maxlength to Link title":
              "https://www.drupal.org/files/issues/2794421-6.patch"
            },
            "drupal/conditional_fields": {
              "2834358 - Uninitialized string offset: 0 in conditional_fields_evaluate_dependency()":
              "https://www.drupal.org/files/issues/conditional_fields-uninitialized_string_offset_in_conditional_fields_evaluate_dependency-2834358-11.patch"
            },
            "drupal/paragraphs": {
              "2764681 - Allow paragraph type deletion if content exists":
              "https://www.drupal.org/files/issues/2018-05-31/allow_paragraphs_deletion-2764681-88.patch"
            }
        }
    },
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "1": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "ucdavis": {
            "type": "vcs",
            "url": "https://github.com/ucdavis/sitefarm_seed.git"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "autoload": {
        "psr-4": {
            "Drupal\\Core\\Composer\\": "docroot/core/lib/Drupal/Core/Composer"
        }
    }
}
